    "build:multi": "webpack --config ./multi-entry-points/entry-points-webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/loader-webpack.config.js",
    "build:plugins": "webpack --config ./plugins/plugin-webpack.config.js",
    "build:dev": "webpack serve --config ./webpack-dev-server/dev-server-webpack.config.js",
    "build:babel": "webpack serve --config ./babel/babel-webpack.config.js",
    "build:react": "webpack serve --config ./react/react-webpack.config.js",
    "build:url-loader": "webpack serve --config ./url-loader/url-loader-webpack.config.js",
    "build:pre": "webpack serve --config ./prepro/prepro-webpack.config.js",
    "build:prevent": "webpack --config ./prevent-duplicate/prevent-duplicate-webpack.config.js",
    "build:dll": "webpack --config ./dll/dll-webpack.config.js",
    "build:prevent-fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic:dll": "webpack --config ./dynamic-import/dll-webpack.config.js",
    "build:dynamic": "webpack --config ./dynamic-import/webpack.config.js"
